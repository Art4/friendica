parameters:
    level: 0

    paths:
        - addon/
        - src/

    excludePaths:
        analyse:
            - addon/*/lang/*
            - addon/*/vendor/*

    scanDirectories:
        - mod
        - static
        - vendor
        - view

    dynamicConstantNames:
        - DB_UPDATE_VERSION
